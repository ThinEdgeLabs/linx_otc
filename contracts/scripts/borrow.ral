TxScript Borrow(loanId: ByteVec, lendingMarketplace: LendingMarketplace) {
  let offer = Loan(loanId)
  let collateralTokenId = offer.getCollateralTokenId()
  let collateralAmount = offer.getCollateralAmount()
  lendingMarketplace.borrow{ callerAddress!() -> collateralTokenId: collateralAmount }(loanId)
}