TxScript CreateLoan(
  lendingTokenId: ByteVec,
  collateralTokenId: ByteVec,
  lendingAmount: U256,
  collateralAmount: U256,
  interestRate: U256,
  duration: U256,
  marketplace: LendingMarketplace
) {
  let _ = marketplace.createLoan{callerAddress!() -> ALPH: minimalContractDeposit!(), lendingTokenId: lendingAmount}(
    lendingTokenId,
    collateralTokenId,
    lendingAmount,
    collateralAmount,
    interestRate,
    duration
  )
}